#include "imx6q.dtsi"
#include "imx6qdl-tinyrex.dtsi"

/ {
	model = "Fedevel i.MX6 Quad TinyREX Board";
	compatible = "fsl,imx6q-tinyrex", "fsl,imx6q";

	memory {
		reg = <0x10000000 0x80000000>;
	};
};

